    "b2b-types": "github:ChumsInc/b2b-types",
    "chums-types": "github:ChumsInc/chums-types",
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
